{
  "name": "voiture.vue",
  "version": "0.0.0",
  "scripts": {
    "start": "yarn api & vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "api": "node server",
    "test:unit": "vitest --environment jsdom",
    "lint": "yarn lint:eslint && yarn lint:vue-tsc",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:vue-tsc": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "storybook": "yarn api & start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commit": "cz",
    "postinstall": "husky install && yarn rmreact",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "rmreact": "rimraf ./node_modules/@types/react"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.6",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "pinia": "^2.0.13",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@mdx-js/react": "^2.1.1",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.28",
    "@storybook/vue3": "^6.4.22",
    "@tailwindcss/forms": "^0.5.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.26",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-storybook": "^0.5.9",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.8",
    "pinst": "^3.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "storybook-dark-mode": "^1.0.9",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.1",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.33.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.2.0"
}
